{
  "name": "hubot-rocketchat-standup",
  "version": "0.0.0-develop",
  "description": "A Hubot for Initiating Virtual Stand Ups",
  "main": "bin/hubot",
  "repository": "git@github.com:RocketChat/hubot-rocketchat-boilerplate.git",
  "author": "Cara Reisle creisle@bcgsc.ca",
  "license": "MIT",
  "private": false,
  "keywords": [
    "hubot",
    "rocketchat",
    "chatbot",
    "chat"
  ],
  "engines": {
    "node": ">=8.x",
    "npm": ">=5.x"
  },
  "scripts": {
    "debug": "node --inspect --inspect-brk -r dotenv/config node_modules/hubot/bin/hubot.js -a rocketchat",
    "shell": "node -r dotenv/config node_modules/hubot/bin/hubot.js -a shell",
    "local": "node -r dotenv/config node_modules/hubot/bin/hubot.js -a rocketchat",
    "start": "source ./.env; hubot -a rocketchat"
  },
  "dependencies": {
    "coffeescript": "^2.2.2",
    "hubot": "3",
    "hubot-conversation": "^1.1.1",
    "hubot-rocketchat": "^2.0.0",
    "hubot-standup-alarm": "^0.1.0",
    "node-schedule": "^1.3.2"
  },
  "devDependencies": {
    "dotenv": "^5.0.1"
  }
}
